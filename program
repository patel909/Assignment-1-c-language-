% ---------------- PROGRAM 1 ----------------
PROGRAM 1: Add two numbers
\begin{lstlisting}
#include <stdio.h>
int main() {
    int x, y, result;
    printf("Enter two integers: ");
    scanf("%d %d", &x, &y);
    result = x + y;
    printf("Addition: %d + %d = %d\n", x, y, result);
    return 0;
}
\end{lstlisting}
\maketitle Output 1
\begin{lstlisting}
Enter two integers: 10 20
Addition: 10 + 20 = 30
=== Code Execution Successful ===
\end{lstlisting}
% ---------------- PROGRAM 2 ----------------
PROGRAM 2: Subtract two numbers
\begin{lstlisting}
#include <stdio.h>
int main() {
    int num1, num2, diff;
    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);
    diff = num1 - num2;
    printf("Result: %d - %d = %d\n", num1, num2, diff);
    return 0;
}
\end{lstlisting}
\maketitle Output 2
\begin{lstlisting}
Enter two integers: 15 8
Result: 15 - 8 = 7
=== Code Execution Successful ===
\end{lstlisting}
% ---------------- PROGRAM 3 ----------------
PROGRAM 3: Multiply two numbers
\begin{lstlisting}
#include <stdio.h>
int main() {
    int p, q;
    printf("Provide two whole numbers: ");
    scanf("%d %d", &p, &q);
    printf("Multiplication: %d x %d = %d\n", p, q, p*q);
    return 0;
}
\end{lstlisting}
\maketitle Output 3
\begin{lstlisting}
Provide two whole numbers: 12 5
Multiplication: 12 x 5 = 60
=== Code Execution Successful ===
\end{lstlisting}
% ---------------- PROGRAM 4 ----------------
PROGRAM 4: Divide two numbers
\begin{lstlisting}
#include <stdio.h>
int main() {
    int dividend, divisor;
    float quotient;
    printf("Enter dividend and divisor: ");
    scanf("%d %d", &dividend, &divisor);
    quotient = (float) dividend / divisor;
    printf("Division result: %.2f\n", quotient);
    return 0;
}
\end{lstlisting}
\maketitle Output 4
\begin{lstlisting}
Enter dividend and divisor: 50 10
Division result: 5.00
=== Code Execution Successful ===
\end{lstlisting}
% ---------------- PROGRAM 5 ----------------
PROGRAM 5: Perform all four operations
\begin{lstlisting}
#include <stdio.h>
int main() {
    int a, b;
    printf("Enter two integers: ");
    scanf("%d %d", &a, &b);
    printf("Sum = %d\n", a+b);
    printf("Difference = %d\n", a-b);
    printf("Product = %d\n", a*b);
    printf("Quotient = %.2f\n", (float)a/b);
    return 0;
}
\end{lstlisting}
\maketitle Output 5
\begin{lstlisting}
Enter two integers: 20 4
Sum = 24
Difference = 16
Product = 80
Quotient = 5.00
=== Code Execution Successful ===
\end{lstlisting}
% ---------------- PROGRAM 6 ----------------
